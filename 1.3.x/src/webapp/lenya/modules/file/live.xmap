<?xml version="1.0" encoding="UTF-8"?>
<!-- File Module - Live -->
<map:sitemap xmlns:map="http://apache.org/cocoon/sitemap/1.0">
  <map:flow language="javascript">
     <map:script src="module:///code.js"/>
  </map:flow>
   <map:pipelines>
      <map:pipeline type="noncaching">
         <map:match pattern="*/**">
            <!-- Send the file -->
            <map:read src="content:/{2}"/>
         </map:match>
      </map:pipeline>
   </map:pipelines>
</map:sitemap>
