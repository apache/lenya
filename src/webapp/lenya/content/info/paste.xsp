<?xml version="1.0"?>

<xsp:page 
  language="java" 
  xmlns:xsp="http://apache.org/xsp"
  xmlns:xsp-request="http://apache.org/xsp/request/2.0"
  xmlns:xsp-cookie="http://apache.org/xsp/cookie/2.0"
  xmlns:xsp-session="http://apache.org/xsp/session/2.0"
  create-session="true">
  
  <info>
    <first-document-id><xsp-session:get-attribute name="org.apache.lenya.cms.info.documentid"/></first-document-id>
    <sec-document-id><xsp-cookie:getValue name="org.apache.lenya.cms.info.targett2"/></sec-document-id>
    <task-id><xsp-session:get-attribute name="org.apache.lenya.cms.info.action"/></task-id>
    <request-uri><xsp-request:get-uri/></request-uri>
  </info>

</xsp:page>