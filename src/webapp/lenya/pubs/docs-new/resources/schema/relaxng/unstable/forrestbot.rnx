datatypes xsd = "http://www.w3.org/2001/XMLSchema-datatypes"

include "any.rnx"

start =             forrest-config
forrest-config =    element forrest-config {
                      defaults,
                      any
                    }

defaults =          element defaults {
                      element * {
                        attribute type {text}?,
                        element * { any }*
                      }*
                    }
