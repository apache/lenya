    <abstract contentEditable="true">
      <xsl:for-each select=".">
        <xsl:apply-templates/>
      </xsl:for-each>
    </abstract>
  </xsl:template>

 <xsl:template match="media-caption">
    <media-caption contentEditable="true">
      <xsl:for-each select=".">
        <xsl:apply-templates/>
      </xsl:for-each>
    </media-caption>
  </xsl:template>
  
  <xsl:template match="*">
        <xsl:copy>
            <xsl:for-each select="@*">
                <xsl:copy/>
            </xsl:for-each>
            <xsl:apply-templates select="node()"/>
        </xsl:copy>
</xsl:template>


</xsl:stylesheet>
