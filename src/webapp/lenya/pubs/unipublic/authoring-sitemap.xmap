					<map:part src="cocoon:/{1}/{2}/{3}/{4}/index.xml" element="cmsbody"/>
				</map:aggregate>
				<map:transform src="xslt/article/xinclude.xsl"/>
				<map:transform type="xlink">
					<map:parameter name="href" value="content/authoring/{1}/{2}/{3}/{4}/index.xml"/>
				</map:transform>
				<map:serialize type="xml"/>
			</map:match>
