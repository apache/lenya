			<!-- select dossier screen -->
			<map:match pattern="selectDossierScreen">
				<map:act type="request">
					<map:parameter name="parameters" value="true"/>
					<map:aggregate element="dossiers">
						<map:part src="cocoon:/select-dossier-dossiers"/>
						<map:part src="cocoon:/{article-file}"/>
					</map:aggregate>
				<map:transform src="lenya/xslt/dossier/select-dossier.xsl">
					<map:parameter name="articlefile" value="{article-file}"/>
				</map:transform>
				</map:act>
				<!--        <map:serialize type="xml"/>-->
				<map:serialize type="html"/>
			</map:match>
			<!-- select dossier -->
			<map:match pattern="selectDossier">
				<map:act type="dossier">
					<map:redirect-to uri="{parent_uri}" session="true"/>
				</map:act>
			</map:match>
		</map:pipeline>
		<!-- dossier selection -->
		<map:pipeline internal-only="true">
			<map:match pattern="select-dossier-dossiers">
				<map:generate type="serverpages" src="content/dossiers/dossier-list.xsp">
					<map:parameter name="dossiers-path" value="content/authoring/dossiers"/>
				</map:generate>
				<map:serialize type="xml"/>
			</map:match>
		</map:pipeline>
		<!-- Samples for resource/document creation -->
		<map:pipeline>
			<map:match pattern="doctypes/samples/*.xml">
				<map:generate src="config/doctypes/samples/{1}.xml"/>
				<map:serialize type="xml"/>
			</map:match>
		</map:pipeline>
		<!-- XML -->
		<map:pipeline>
			<map:match pattern="**.xml">
				<map:generate src="content/authoring/{1}.xml"/>
				<map:serialize type="xml"/>
			</map:match>
		</map:pipeline>
	</map:pipelines>
</map:sitemap>
