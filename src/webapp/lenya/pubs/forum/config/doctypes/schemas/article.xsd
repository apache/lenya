<Schema xmlns="urn:schemas-microsoft-com:xml-data" xmlns:dt="urn:schemas-microsoft-com:datatypes">

<ElementType content="eltOnly" model="closed" name="article">
 <group>
  <element type="head" minOccurs="1" maxOccurs="1"/>
  <element type="body" minOccurs="1" maxOccurs="1"/>
 </group>
</ElementType>

<ElementType content="eltOnly" model="closed" name="head">
